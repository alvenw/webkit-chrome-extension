{
  "manifest_version": 2,

  "name": "InvertColorizer",
  "description": "Spices up your computer to invert the colors on your computer!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html" 
	},

	"background": {
  "scripts": ["background.js"],
  "persistent": false
	},

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.1.4.min.js", "content.js"]
  }
	]
}